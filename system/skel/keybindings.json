[
    // --- Majo418 Eclipse ---
    // Majo418's Eclipse keymap is a eclipse ide shortcut keymap.
    // The best of eclipse and vscode.
    //
    // Windows/Linux
    {
        "key": "ctrl+f",
        "command": "actions.find",
        "when": "!isMac && actionsFind"
    },
    {
        "key": "ctrl+y",
        "command": "redo",
        "when": "!isMac && textInputFocus && !editorReadonly"
    },
    {
        "key": "ctrl+numpad_divide",
        "command": "editor.fold",
        "when": "!isMac && editorFocus"
    },
    {
        "key": "ctrl+numpad_multiply",
        "command": "editor.unfold",
        "when": "!isMac && editorFocus"
    },
    {
        "key": "ctrl+shift+numpad_divide",
        "command": "editor.foldAll",
        "when": "!isMac && editorFocus"
    },
    {
        "key": "ctrl+shift+numpad_multiply",
        "command": "editor.unfoldAll",
        "when": "!isMac && editorFocus"
    },
    {
        "key": "ctrl+3",
        "command": "workbench.action.showCommands",
        "when": "!isMac"
    },
    {
        "key": "ctrl+shift+r",
        "command": "workbench.action.quickOpen",
        "when": "!isMac"
    },
    {
        "key": "ctrl+h",
        "command": "workbench.view.search",
        "when": "!isMac"
    },
    {
        "key": "ctrl+shift+t",
        "command": "workbench.action.showAllSymbols",
        "when": "!isMac"
    },
    {
        "key": "ctrl+l",
        "command": "workbench.action.gotoLine",
        "when": "!isMac"
    },
    {
        "key": "ctrl+w",
        "command": "workbench.action.closeActiveEditor",
        "when": "!isMac"
    },
    {
        "key": "ctrl+shift+s",
        "command": "workbench.action.files.saveAll",
        "when": "!isMac"
    },
    {
        "key": "ctrl+alt+s",
        "command": "workbench.action.files.saveAs",
        "when": "!isMac"
    },
    {
        "key": "ctrl+alt+t",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "!isMac"
    },
    {
        "key": "ctrl+m",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "!isMac"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.openPreviousRecentlyUsedEditorInGroup",
        "when": "!isMac"
    },
    {
        "key": "ctrl+shift+w",
        "command": "workbench.action.closeAllEditors",
        "when": "!isMac"
    },
    {
        "key": "ctrl+shift+f4",
        "command": "workbench.action.closeAllEditors",
        "when": "!isMac"
    },
    {
        "key": "ctrl+f12",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "!isMac"
    },
    {
        "key": "ctrl+shift+c",
        "command": "editor.action.commentLine",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "ctrl+shift+/",
        "command": "editor.action.blockComment",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "ctrl+alt+y",
        "command": "editor.action.toggleWordWrap",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "ctrl+alt+j",
        "command": "editor.action.joinLines",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.gotoSymbol",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "ctrl+shift+f",
        "command": "editor.action.formatSelection",
        "when": "!isMac && editorTextFocus && editorHasSelection"
    },
    {
        "key": "ctrl+shift+f",
        "command": "editor.action.formatDocument",
        "when": "!isMac && editorTextFocus && !editorHasSelection"
    },
    {
        "key": "cmd+shift+down",
        "command": "editor.action.copyLinesDownAction",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "cmd+shift+up",
        "command": "editor.action.copyLinesUpAction",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "alt+down",
        "command": "editor.action.moveLinesDownAction",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "alt+up",
        "command": "editor.action.moveLinesUpAction",
        "when": "!isMac && editorTextFocus"
    },
    {
        "key": "ctrl+d",
        "command": "editor.action.deleteLines",
        "when": "!isMac && editorTextFocus"

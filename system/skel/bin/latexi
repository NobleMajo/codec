#!/usr/bin/env bash

echo " ### Install latex deps..."
sudo -E apt-get install -y \
    latexmk biber \
    texlive-bibtex-extra texlive-lang-german \
    texlive texlive-lang-european texlive-pictures \
    texlive-latex-base texlive-latex-extra \
    texlive-fonts-recommended texlive-fonts-extra

echo " ### Finished!"